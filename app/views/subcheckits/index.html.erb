<table class="table table-hover table-striped table-condensed">
  <thead>
    <tr>
      <th>SubCheckit</th>
      <th>Category</th>
    </tr>
  </thead>
  <tbody>
    <% @subcheckits.each do |subcheckit| %>
      <tr>
        <td><%= link_to subcheckit.name, subcheckit_path(subcheckit.id) %></td>
        <td><%= subcheckit.category %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New SubCheckit", new_subcheckit_path, class: "btn btn-success"  %>
